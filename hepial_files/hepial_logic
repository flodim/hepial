debutprg

    si 1 == 23 ou 1==1 alors
	    ecrire "true";
	sinon
	    ecrire "false";
	finsi

	si 4 == 2+2 et 12 == 3*4 alors
        ecrire "true";
    sinon
        ecrire "false";
    finsi

